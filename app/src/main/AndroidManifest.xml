<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.shileiyu.jetpack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.TabActivity" />
        <activity android:name=".ui.home.CategoryActivity" />
        <activity android:name=".ui.AreaViewActivity" />
        <activity android:name=".ui.SeatActivity" />
        <activity android:name=".ui.MatrixActivity" />
        <activity android:name=".ui.TestActivity"></activity>
        <activity android:name=".ui.DragActivity" />
        <activity android:name=".ui.MoveActivity"></activity>
    </application>

</manifest>